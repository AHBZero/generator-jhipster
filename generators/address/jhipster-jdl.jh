entity Country {
	name String,
    acronym String
}

entity State {
	name String,
    acronym String
}

entity City {
	name String
}

entity Address {
	street String,
    number String,
    zipcode String,
}

entity Phone {
	areaCode Integer,
    number String,
    type PhoneType
}

enum PhoneType {
	HOME, PROFESSIONAL, CELLULAR, OTHER
}

relationship OneToMany {
	State{country} to Country,
    City{state} to State
}

relationship ManyToMany {
	Address{city} to City
}



